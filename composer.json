{
  "name": "dameert/frontend-cms",
  "description": "Simple CMS for small projects with file based content storage and frontend editor for updates",
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "David Meert",
      "email": "david@davidmeert.be"
    }
  ],
  "require": {
    "php": ">=5.6",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/orm": "^2.5",
    "symfony/symfony": "3.3.*",
    "twig/twig": "^1.0||^2.0",
    "symfony/assetic-bundle": "^2.8"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "Dameert\\FrontendCms\\": "src/"
    },
    "exclude-from-classmap": [
      "/src/Tests/"
    ]
  },
  "extra" : {
    "branch-alias" : {
      "dev-master" : "0.1-dev"
    }
  }
}
